{
    "name": "Online Clipbaord",
    "version": "0.1.0",
    "manifest_version": 3,
    "description": "A simple online clipboard",
    "permissions": ["storage", "tabs", "clipboardWrite", "clipboardRead"],
    "host_permissions": ["*://*/*"],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    }


}